<h1>Admin Page</h1>
{{ dump(users) }}
<table>
    <tr>
        <th>Username</th>
        <th>Email</th>
        <th>Role</th>
        <th>Last Login</th>
        <th>Actions</th>
    </tr>
    {% for user in users %}
        <tr>
            <td>{{ user.username }}</td>
            <td>{{ user.email }}</td>
            {% for role in user.roles %}
                <td>{{ role }}</td>
            {% endfor %}
            <td>{{ user.lastLogin is empty ? "n/a" : user.lastLogin.date is empty ? "n/a" : user.lastLogin.date | date("d/m/Y G:i:s e") }}</td>
        </tr>
    {% endfor %}
</table>